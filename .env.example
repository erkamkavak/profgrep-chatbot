AUTH_SECRET=YOUR_AUTH_SECRET
DATABASE_URL=

# Primary model provider (OpenRouter, used for chat and tools)
OPENROUTER_API_KEY=sk-or-...

# Google OAuth (or switch to GitHub by using AUTH_GITHUB_* instead)
AUTH_GOOGLE_ID=YOUR_GOOGLE_OAUTH_CLIENT_ID
AUTH_GOOGLE_SECRET=YOUR_GOOGLE_OAUTH_CLIENT_SECRET

# GitHub OAuth (optional alternative auth provider)
AUTH_GITHUB_ID=
AUTH_GITHUB_SECRET=

# Mixedbread for mgrep semantic search
MIXEDBREAD_API_KEY=mxb_...
# Optional: base store name for Mixedbread (defaults to "mgrep" if unset)
MXBAI_STORE=mgrep

# --- Observability (Langfuse, optional) ---
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_BASE_URL=https://cloud.langfuse.com

# [optional] Cleanup cron job secret
CRON_SECRET=

# --- Optional features from original Sparka template ---
# E2B Code Interpreter - https://e2b.dev/docs
E2B_API_KEY=
SANDBOX_TEMPLATE_ID=

# Redis - for resumable streams
REDIS_URL=

# OpenAI, Tavily, Exa, Firecrawl (not required for core professor search)
OPENAI_API_KEY=
TAVILY_API_KEY=
EXA_API_KEY=
FIRECRAWL_API_KEY=
